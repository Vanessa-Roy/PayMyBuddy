spring.datasource.url=jdbc:mysql://localhost:3306/PayMyBuddy?serverTimeZone=UTC
spring.datasource.username=root
spring.datasource.password=yourPassword

spring.security.oauth2.client.registration.github.client-id=yourClientId
spring.security.oauth2.client.registration.github.client-secret=yourClientSecret
spring.security.oauth2.client.registration.github.scope=user

spring.jpa.properties.javax.persistence.schema-generation.scripts.action=create
spring.jpa.properties.javax.persistence.schema-generation.scripts.create-target=create.sql
spring.jpa.properties.javax.persistence.schema-generation.scripts.create-source=metadata

spring.jpa.hibernate.ddl-auto = update
spring.mvc.view.suffix=.html